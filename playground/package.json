{
  "name": "playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prebuild": "npm run copy:worker && npm run copy:monarch-syntax",
    "copy:worker": "shx cp -fr ../out/language/statetree-server-worker.js ./libs/statetree-server-worker.js",
    "copy:monarch-syntax": "shx cp -fr ../out/syntaxes/ ./libs/syntaxes",
    "start": "npx serve dist",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.5.1",
    "daisyui": "^3.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/standalone": "^7.22.5",
    "@babel/traverse": "^7.22.5",
    "@codemirror/commands": "^6.2.4",
    "@codemirror/lang-javascript": "^6.1.9",
    "@codemirror/state": "^6.2.1",
    "@codemirror/view": "^6.13.2",
    "@codesandbox/sandpack-react": "^2.6.7",
    "@emmetio/codemirror6-plugin": "^0.3.0",
    "@heroicons/react": "^2.0.18",
    "@nanostores/react": "^0.7.1",
    "@radix-ui/react-slot": "^1.0.2",
    "@typefox/monaco-editor-react": "^1.0.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@uiw/codemirror-theme-atomone": "^4.21.3",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "codemirror": "^6.0.1",
    "constate": "^3.3.2",
    "daisyui": "^3.1.6",
    "dockview": "^1.7.6",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "monaco-editor-workers": "^0.38.0",
    "monaco-editor-wrapper": "^2.0.1",
    "monaco-jsx-highlighter": "^2.0.4",
    "monaco-jsx-syntax-highlight": "^1.2.0",
    "nanostores": "^0.9.2",
    "postcss": "^8.4.24",
    "react-error-boundary": "^4.0.10",
    "react-inspector": "^6.0.2",
    "sass": "^1.63.4",
    "tailwind-merge": "^1.13.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "use-debounce": "^9.0.4",
    "use-undo": "^1.1.1",
    "vite": "^4.3.9",
    "vite-plugin-monaco-editor": "^1.1.0"
  }
}
